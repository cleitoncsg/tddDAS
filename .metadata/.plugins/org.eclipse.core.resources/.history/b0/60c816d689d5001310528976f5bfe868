import org.junit.Test;

public class testFuncionario {
	
	private Funcionario f;

	@Before
	public void setUp() throws Exception {
		f = new Funcionario();
	}

	@Test
	public void calculaSalarioFuncionarioTest() {
		assertEquals(10, f.calculoSalario(), 0.1);
	}

